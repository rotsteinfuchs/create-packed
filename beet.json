{
  "name": "Create: Packed",
  "version": "1.0.0",
  "description": "The Create Mod - but packed into a data pack!",
  "author": "Rotsteinfuchs",
  "output": "build",
  "data_pack": {
    "load": ["src"],
    "render": {
      "functions": ["*"]
    }
  },
  "resource_pack": {
    "load": ["src"]
  },
  "pipeline": ["build.main"],
  "meta": {
    "generate_namespace": "create_packed"
  }
}
